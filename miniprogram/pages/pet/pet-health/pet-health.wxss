page {
  background-color: #fff;
  color: #333;
}

.init {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

/* 未开启的健康提醒页面 */
.project-wrapper {
  padding: 20rpx;
}

.project-name {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  padding: 35rpx;
}
.project-name .iconfont {
  display: block;

  width: 95rpx;
  height: 95rpx;
  padding: 5rpx;
  padding-left: 10rpx;
  border-radius: 50%;
  border: 3rpx solid #fff;

  font-size: 75rpx;
  color: #fff;
}

.project-item .title {
  padding: 10rpx 0;
  font-size: 30rpx;
  font-weight: bold;
  background-color: #f7f7f7;
}
.project-item .content {
  padding: 12rpx;
  font-size: 32rpx;
}
.content-item {
  display: flex;
  border-bottom: 1rpx solid #f7f7f7;
  padding: 15rpx 8rpx;
}
.content-item .item-left {
  width: 60%;
}
.content-item .item-right {
  display: flex;
  margin-left: auto;
}
.icon-xuanzeqixiayige_o {
  margin: 0;
  margin-left: 15rpx;
  color: #ccc;
  font-size: 45rpx;
}

/* 按钮样式 */
.btn-group {
  position: fixed;
  width: 100%;
  bottom: 0;
}
.open-btn,.cancel-btn, .return-btn {
  margin: 10rpx 0;
  font-weight: normal;
  font-size: 38rpx;
  color: #fff;
}
